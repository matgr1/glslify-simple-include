{
	"compilerOptions": {
		"noEmit": false,
		"outDir": "../build/es6",
        "rootDir": "./",
		"inlineSourceMap": true,
		"inlineSources": true,
		"declaration": true,
		"declarationDir": "../build",
		"noImplicitReturns": true,
		"noImplicitAny": true,
		"noEmitOnError": true,
		"removeComments": false,
		"target": "es6",
		"module": "commonjs",
		"allowSyntheticDefaultImports": true
	}
}
